class Animal{
	void eat(){
		System.out.println("Eating the food");
	}

}
class Horse extends Animal{
	void eat(){
		System.out.println("Horse is Eating the hay");
	}

}

class test{
	public static void main(String [] args){
		Animal a=new Animal();
		Horse h=new Horse();
		a.eat();
		h.eat();
		
	}

}



